<dl class="see after_move">
  <dt><%= t "chorg.views.#{params[:type]}_changesets.after_move" %></dt>
  <dd></dd>
  <% Chorg::Changeset::GROUP_ATTRIBUTES.each do |k| %>
    <dt><%= t "chorg.views.#{params[:type]}_changesets.#{k}" %></dt>
    <dd><%= @item.destinations.try(:first).try(:send, :[], k) %></dd>
  <% end %>
</dl>

<dl class="see before_move">
  <dt><%= t "chorg.views.#{params[:type]}_changesets.before_move" %></dt>
  <dd></dd>
  <dt><%= t "chorg.views.#{params[:type]}_changesets.name" %></dt>
  <dd><%= @item.sources.try(:first).try(:send, :[], "name") %></dd>
</dl>
